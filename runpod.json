{
    "name": "ComfyUI Faceswap Runner",
    "version": "1.0.0",
    "description": "RunPod API pour faceswap avec ComfyUI",
    "handler": "handler.py",
    "container": {
      "gpu": 1
    },
    "runtime": "python3",
    "runtimeContext": {
      "timeoutSeconds": 300,
      "memoryGB": 16
    },
    "healthCheck": {
      "type": "http",
      "route": "/health"
    }
  }